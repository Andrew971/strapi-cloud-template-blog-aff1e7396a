{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": "Create your blog content"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titre": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "titre"
    },
    "stock": {
      "type": "boolean",
      "default": true
    },
    "prix": {
      "type": "decimal"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "shared.media",
        "shared.quote",
        "shared.rich-text",
        "shared.slider"
      ]
    },
    "description": {
      "type": "text"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "volume",
        "poids",
        "parfum",
        "format"
      ]
    },
    "valeur": {
      "type": "string"
    }
  }
}
