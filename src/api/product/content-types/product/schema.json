{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titre": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "titre"
    },
    "poster": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "gallerie": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article"
    },
    "catalogues": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::catalogue.catalogue",
      "mappedBy": "product"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "product"
    },
    "rank": {
      "type": "json"
    },
    "technicalInformation": {
      "type": "component",
      "component": "product.technical-information",
      "repeatable": false
    },
    "productInformation": {
      "type": "component",
      "component": "product.product-information",
      "repeatable": true
    }
  }
}
